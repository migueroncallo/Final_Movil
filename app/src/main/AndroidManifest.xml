<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uninorte.migueroncallo.rotacion" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
    <uses-permission android:name="com.android.launcher.permission.UNINSTALL_SHORTCUT" />

    <application
        android:name="com.uninorte.migueroncallo.rotacion.parseInitializer"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.Sign_up"
            android:label="@string/title_activity_sign_up" >
        </activity>
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.Inicio"
            android:label="@string/title_activity_inicio" >
        </activity>
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.ver_cursos"
            android:label="@string/title_activity_ver_cursos" >
        </activity>
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.entrar_curso"
            android:label="@string/title_activity_entrar_curso" >
        </activity>
        <activity
            android:name="com.uninorte.migueroncallo.rotacion.perfil_estud"
            android:label="@string/title_activity_perfil_estud" >
        </activity>
    </application>

</manifest>
